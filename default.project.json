{
  "name": "roblecs",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Source": { "$path": "src/Kernel/Source" },
      "Modules" : { "$path": "src/Modules" },
      "Assets": { "$path": "src/Game/Shared/Assets" },
      "Components": { "$path": "src/Game/Shared/Components" },
      "Events": { "$path": "src/Game/Shared/Events" },
      "GameEventManager": { "$className": "RemoteEvent" }
    },
    "ServerScriptService": {
      "Server": { "$path": "src/Game/Server" },
      "EntryPoint": { "$path": "src/Kernel/Server" }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": { "$path": "src/Game/Client" },
        "EntryPoint": { "$path": "src/Kernel/Client" }
      }
    }
  }
}